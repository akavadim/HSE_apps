@model IEnumerable<Arnet.Database.Apartment>
@{
    ViewData["Title"] = "Моя недвижимость";
}

<a class="btn btn-primary" asp-action="AddApartment">Добавить</a>
<table class="table">
    <tbody>
        @foreach (var apartment in Model)
        {
            <tr>
                <td>@apartment.ApartmentId</td>
                <td><a href="@Url.Action("Index", "Apartment", new { id=apartment.ApartmentId})">Просмотреть</a></td>
            </tr>
        }
    </tbody>
</table>

