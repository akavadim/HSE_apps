@model Arnet.Web.Areas.Account.Models.Registration.AccountModel

<div class="field">
    <div class="field-name">
        @Html.DisplayNameFor(m => m.SecondName)
    </div>
    <div class="field-content">
        @Html.EditorFor(m => m.SecondName, new{ htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(m => m.SecondName)
    </div>
</div>
<div class="field">
    <div class="field-name">
        @Html.DisplayNameFor(m => m.FirstName)
    </div>
    <div class="field-content">
        @Html.EditorFor(m => m.FirstName,  new{ htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(m => m.FirstName)
    </div>
</div>
<div class="field">
    <div class="field-name">
        @Html.DisplayNameFor(m => m.Email)
    </div>
    <div class="field-content">
        @Html.EditorFor(m => m.Email,  new{ htmlAttributes = new { @class = "form-control" } })
        @Html.ValidationMessageFor(m => m.Email)
    </div>
</div>
<div class="field">
    <div class="field-name">
        @Html.DisplayNameFor(m => m.Password)
    </div>
    <div class="field-content">
        <input type="password" asp-for="Password" class="form-control"/>
        @Html.ValidationMessageFor(m => m.Password)
    </div>
</div>